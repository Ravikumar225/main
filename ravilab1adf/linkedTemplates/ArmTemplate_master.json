{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ravilab1adf"
		},
		"getsourcedtLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'getsourcedtLS'"
		},
		"sourcelab1LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sourcelab1LS'"
		},
		"targetlab1sqlLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'targetlab1sqlLS'"
		},
		"getsourcedtLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ravilab1st.dfs.core.windows.net/"
		},
		"sourcelab1LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ravilab1st.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ravilab1adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"getsourcedtLS_accountKey": {
						"value": "[parameters('getsourcedtLS_accountKey')]"
					},
					"sourcelab1LS_accountKey": {
						"value": "[parameters('sourcelab1LS_accountKey')]"
					},
					"targetlab1sqlLS_connectionString": {
						"value": "[parameters('targetlab1sqlLS_connectionString')]"
					},
					"getsourcedtLS_properties_typeProperties_url": {
						"value": "[parameters('getsourcedtLS_properties_typeProperties_url')]"
					},
					"sourcelab1LS_properties_typeProperties_url": {
						"value": "[parameters('sourcelab1LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}